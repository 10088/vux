    "serve": "vite preview",
    "test": "jest"
  },
  "dependencies": {
    "typescript": "^4.3.2",
    "vue": "^3.1.1",
    "vue-router": "^4.0.9"
  },
  "devDependencies": {
    "@babel/node": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.1",
    "@vue/test-utils": "^2.0.0-rc.4",
    "babel-jest": "^26.6.3",
    "chalk": "^4.1.1",
    "cross-env": "^7.0.3",
    "fast-glob": "^3.2.5",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "inquirer": "^8.1.0",
    "jest": "^26.6.3",
    "less": "^4.1.1",
    "ora": "^5.4.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.35.1",
    "ts-jest": "^26.5.4",
    "vite": "^2.3.7",
    "vite-plugin-md": "^0.6.7",
    "vite-plugin-vuedoc": "^3.1.3",
    "vue-jest": "^5.0.0-alpha.7"
